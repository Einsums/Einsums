{
    "name": "einsums",
    "image": "jetturney/code-dev:latest",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined",
        "--userns=host"
    ],
    "settings": {
        "terminal.integrated": {
            "profiles.linux": {
                "zsh": {
                    "path": "zsh"
                }
            },
            "defaultProfile.linux": "zsh"
        },
        "files.eol": "\n",
        "resmon.show.battery": false,
        "resmon.show.cpufreq": false
    },
    "remoteUser": "vscode",
    "extensions": [
        "ms-vscode.cmake-tools",
        "twxs.cmake",
        "llvm-vs-code-extensions.vscode-clangd",
        "mutantdino.resourcemonitor",
    ]
}